<!-- Modal -->
<div class="modal fade" id="addPropertyModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span>
                </button>
                <h4 class="modal-title" id="myModalLabel">Modal title</h4>
            </div>
            <div class="modal-body">
                <form name="prop_form" id="prop_form">
                    <fieldset class="form-group">
                        <label><h4>Use Code</h4></label>
                        <select class="form-control" name="use_code" title="Use Code" required>
                            {{#each use_code as |use|}}
                                <option label="{{use.name}}" value="{{use.code}}"></option>
                            {{/each}}
                        </select>
                    </fieldset>
                    <fieldset class="form-group">
                        <label><h4>Sanitation Code</h4></label>
                        <select class="form-control" id="sanitation_code" name="sanitation_code" title="Sanitation Code" required>
                            {{#each sanitation as |san|}}
                                <option label="{{san.name}}" value="{{san.code}}"></option>
                            {{/each}}
                        </select>
                    </fieldset>
                    <fieldset class="form-group">
                        <label><h4>Property Number</h4></label>
                        <input class="form-control" name="prop_num" required>
                    </fieldset>
                    <fieldset class="form-group">
                        <label><h4>Area</h4></label>
                        <input class="form-control" id="area_bar">
                        <input class="form-control" name="area" placeholder="Use JQuery autocomplete" required readonly id="are_a">
                    </fieldset>
                    <fieldset class="form-group">
                        <label><h4>Location</h4></label>
                        <input class="form-control" name="x" placeholder="x" type="number" step="0.0001" required>
                        <input class="form-control" name="y" placeholder="y" type="number" step="0.0001" required>
                        <input class="form-control" name="loc_des" placeholder="location description" required>
                    </fieldset>
                    <fieldset class="form-group">
                        <label><h4>Owner ID</h4></label>
                        <input class="form-control" name="owner" readonly value="{{owner._id}}">
                    </fieldset>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary" id="add_prop">Save changes</button>
            </div>
        </div>
    </div>
</div>